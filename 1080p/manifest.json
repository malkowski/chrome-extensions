{
  "manifest_version": 3,
  "name": "1080p Resizer",
  "version": "1.0",
  "description": "Adds a button to resize the window to 1920x1080",
  "permissions": [
    "windows"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "1080p"
  }
}